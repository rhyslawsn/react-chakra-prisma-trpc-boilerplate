    "start": "node build/main.js",
    "dev": "SET NODE_ENV=development && nodemon src/main.ts",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint -- --fix --ext .ts ."
  },
  "dependencies": {
    "@prisma/client": "5.12.1",
    "@supabase/supabase-js": "2.42.3",
    "@trpc/server": "10.45.2",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "csv-reader": "1.0.12",
    "date-fns": "3.6.0",
    "date-fns-tz": "3.0.1",
    "dotenv": "16.4.5",
    "lodash": "4.17.21",
    "prisma": "5.12.1",
    "trpc-shield": "0.4.4",
    "tsconfig": "*",
    "tsup": "8.1.0",
    "util": "0.12.5",
    "uuid": "9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/lodash": "4.17.0",
    "@types/node": "20.11.5",
    "@types/node-cron": "3.0.11",
    "@types/uuid": "9.0.8",
    "csv-parser": "3.0.0",
    "nodemon": "3.1.0",
    "playwright": "1.43.0",
    "ts-node": "10.9.2",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "deasync": "0.1.29"
  },
  "engines": {
    "node": "<=20.11.0"
  }
