{
  "name": "@now-stroke-it/api",
  "version": "1.0.0",
  "private": true,
  "description": "API for recipe-to-grocery",
  "type": "commonjs",
  "exports": {
    ".": "./src/main.ts"
  },
  "main": "build/main.js",
  "module": "build/main.mjs",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "yarn build:prisma && tsup-node",
    "build:cleanup": "rm -rf ./build",
